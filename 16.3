#include <iostream>

using namespace std;



int main()

{
	int i1,j1,max, min, s, N;
	int a[1000];
	int b[1000];
	cin >> N;
	for (int i = 1;i <= N;i++)
	{
		cin >> a[i];
	}
	s = 1;
	max = a[1];
	i1 = 1;
	j1 = 1;
	min = a[1];
		for (int i = 1;i <= N;i++)
		{
			if (a[i] > max)
			{
				max = a[i];
				i1 = i;
			}
			if (a[i] < min)
			{
				min = a[i];
				j1 = i;
			}
		}
		for (int i = 1;i <= N;i++)
		{
			if (i == j1)
			{
				b[s] = 0;
				s++;
			}
			b[s] = a[i];
			s++;
			if (i == i1)
			{
				b[s] = 0;
				s++;
			}
		}
		for (int i = 1;i <= N + 2;i++)
		{
			cout << b[i] << " ";
		}
    }
