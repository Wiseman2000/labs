#include <iostream>
#include <string>
#include <clocale>
#include <Windows.h>
using namespace std;
int main()
{
	int k, kol,r;
	bool a;
	string s;
	setlocale(LC_ALL, "Russian");
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
        getline(cin, s);
		kol = 0;
		k = 0;
		while (k < s.length())
		{
			r = 0;
			a = 1;
			while ((a == 1) && (k < s.length()))
			{
				if (s[k] != ' ')
				{
					r++;
					k++;
				}
				else a = 0;
			}
			if (r != 0) kol++;
			k++;
		}
		cout << kol << endl;
}
