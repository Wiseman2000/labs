#include <iostream>
#include <math.h>
#include <iostream>
using namespace std;
int main()
{
	int  i, k, N, M, b;
    int a[100][100];
	cin >> M >> N;
	for (int i = 1;i <= M;i++)
	{
		for (int j = 1;j <= N;j++)
		{
			cin >> a[i][j];
		}
	}
	k = 0;
	i = 1;
	b = 0;
	while ((b==0) && (i<=N))
	{
		b = 1;
		for (int j = 1;j <= M;j++)
		{
			if (a[j][i]%2==0)
			{
				b = 0;
			}
		}
		if (b == 1)
		{
			k = i;
		}
		i++;
	}
	cout << k;
}
