#include <iostream>
#include <math.h>
#include <iostream>
using namespace std;
int main()
{
	int  p, min, max, N, M, s, k;
    int a[100][100];
	cin >> M >> N;
	for (int i = 1;i <= M;i++)
	{
		for (int j = 1;j <= N;j++)
		{
			cin >> a[i][j];
		}
	}
    for (int i=1;i<=M;i++)
	{
		min = a[i][1];
		max = a[i][1];
		k = 1;
		s = 1;
		for (int j = 1;j <= N;j++)
		{
			if (a[i][j] > max)
			{
				max = a[i][j];
				k = j;
			}
			if (a[i][j] < min)
			{
				min = a[i][j];
				s = j;
			}
		}
		p = a[i][k];
		a[i][k] = a[i][s];
		a[i][s] = p;
	}
	for (int i = 1;i <= M;i++)
	{
		for (int j = 1;j <= N;j++)
		{
			cout << a[i][j] << " ";
		}
		cout << endl;
	}
}
