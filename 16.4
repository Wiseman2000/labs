#include <iostream>

using namespace std;



int main()

{
	int s, N;
	int a[1000];
	int b[1000];
	cin >> N;
	for (int i = 1;i <= N;i++)
	{
		cin >> a[i];
	}
	s = 1;
	for (int i = 1;i <= N;i++)
	{
		if (a[i] < 0)
		{
			b[s] = a[i];
			s++;
			b[s] = 0;
			s++;
		}
		else
		{
			b[s] = a[i];
			s++;
		}
	}
	for (int i = 1;i <= s - 1;i++)
	{
		cout << b[i] << " ";
	}
}
