#include <iostream>

using namespace std;



int main()

{

	int k, N;
	int a[1000];
	int b[1000];
	cin >> N;
	for (int i = 1;i <= N;i++)
	{
		cin >> a[i];
	}
	for (int i = 1;i <= N;i++)
	{
		cin >> b[i];
	}
	for (int i = 1; i <= N; i++)
	{
		k = a[i];
		a[i] = b[i];
		b[i] = k;
	}
	for (int i = 1;i <= N;i++)
	{
		cout << a[i] << " ";
	}
	cout << endl;
	for (int i = 1;i <= N;i++)
	{
		cout << b[i] << " ";
	}
}
