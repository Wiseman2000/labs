#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	int  j1, j2,N, k, l, t;
	cin >> N;
	int* a = new int [N];
	int* b = new int [N];
	int* c = new int [N];
	for (int i = 0;i < N;i++)
	{
			cin >> a[i];
	}
	t = 0;
	j1 = 0;
	j2 = 0;
	while (t < N)
	{
		k = 1;
		l = a[t];
		while ((a[t] == a[t + 1])&&(t!=N))
		{
			k++;
			t++;
		}
		b[j1] = k;
		j1++;
		c[j2] = l;
		j2++;
		t++;
	}
	for (int i = 0; i < j1; i++)
	{
		cout << b[i] << " ";
	}
	cout << endl;
	for (int i = 0; i < j2; i++)
	{
		cout << c[i] << " ";
	}
}
