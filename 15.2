#include <iostream>

using namespace std;



int main()

{

	int N;
	float sum;
	float a[1000];
	float b[1000];
	cin >> N;
	for (int i = 1;i <= N;i++)
	{
		cin >> a[i];
	}
	for (int i = 1; i <= N; i++)
	{
		sum = 0;
		for (int j = 1;j <= i;j++)
		{
			sum = sum + a[j];
		}
		b[i] = sum / i;
	}
	for (int i = 1;i <= N;i++)
	{
		cout << b[i] << " ";
	}
}
