
#include <iostream>

using namespace std;



int main()

{
	int min, k, max, h, N;
	int a[1000];
	cin >> N;
	min = 2147483647;
	max = -2147483647;
	for (int i = 1;i <= N;i++)
	{
		cin >> a[i];
		if (a[i] < min)
		{
			min = a[i];
			k = i;
		}
		if (a[i] > max)
		{
			max = a[i];
			h = i;
		}
	}
	if (k > h)
	{
		for (int i = h + 1;i < k;i++)
		{
			a[i] = 0;
		}
	}
	if (k < h)
	{
		for (int i = k + 1;i < h;i++)
		{
			a[i] = 0;
		}
	}
	for (int i = 1;i <= N;i++)
	{
		cout << a[i] << " ";
	}
}
