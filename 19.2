#include <iostream>
#include <math.h>
#include <iostream>
using namespace std;
int main()
{
	int  p, min, max, N, M, s, k;
    int a[100][100];
	cin >> M >> N;
	for (int i = 1;i <= M;i++)
	{
		for (int j = 1;j <= N;j++)
		{
			cin >> a[i][j];
		}
	}
	min = a[1][1];
	max = a[1][1];
	k = 1;
	s = 1;
    for (int i=1;i<=N;i++)
	{
		for (int j = 1;j <= M;j++)
		{
			if (a[j][i] > max)
			{
				max = a[j][i];
				s = i;
			}
			if (a[j][i] < min)
			{
				min = a[j][i];
				k = i;
			}
		}
	}
	for (int i = 1;i <= M;i++)
	{
		p = a[i][k];
		a[i][k] = a[i][s];
		a[i][s] = p;
	}
	for (int i = 1;i <= M;i++)
	{
		for (int j = 1;j <= N;j++)
		{
			cout << a[i][j] << " ";
		}
		cout << endl;
	}
}
