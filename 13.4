#include <iostream>

#include <math.h>

















using namespace std;

int main()

{

	int n;

	cin >> n;

	int mas[3000];
	for (int i = 1;i <= n;i++)
	{
		cin >> mas[i];
	}
	for (int i = 1;i <= n/2;i++)
	{
		cout << mas[i] << " " << mas[n + 1 - i] << " ";
	}
	if (n % 2 == 1)
	{
		cout << mas[n / 2 + 1];
	}
}
